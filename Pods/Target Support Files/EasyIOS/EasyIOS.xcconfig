EASYIOS_CLANG_CXX_LANGUAGE_STANDARD = gnu++0x
EASYIOS_CLANG_CXX_LIBRARY = libstdc++
EASYIOS_CLANG_WARN_DIRECT_OBJC_ISA_USAGE = YES
EASYIOS_HEADER_SEARCH_PATHS = $(SDKROOT)/usr/include/libz, $(SDKROOT)/usr/include/libxml2
EASYIOS_OTHER_LDFLAGS = -l"sqlite3" -l"stdc++" -l"z.1.1.3" -framework "CoreFoundation" -framework "CoreGraphics" -framework "Foundation" -framework "Security" -framework "UIKit"